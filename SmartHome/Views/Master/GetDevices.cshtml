@model SmartHome.Models.DeviceAPI
@{
    ViewBag.Title = "Master";
}
<body color: #ffffff;>
    <div background-color  = "#8f969c">
        <h3>Распродажа книг</h3>
        <table>
            <tr>
                <td><p>Название книги</p></td>
                <td><p>Автор</p></td>
                <td><p>Цена</p></td>
                <td></td>
            </tr>
            @foreach (var b in ViewBag.list)
            {
                <tr>
                    <td><p>@b.ID</p></td>
                    <td><p>@b.Type</p></td>
                    <td><p>@b.Value</p></td>
                </tr>
            }
        </table>
    </div>
</body>
